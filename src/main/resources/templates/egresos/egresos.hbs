{{#partial "contenido"}}
<h1 style="text-decoration:underline;">Egresos</h1>

	<table align="center" border="5">
		<tr>
			<th>EGRESO</th>
			<th>PROVEEDOR</th>
			<th>DOCUMENTO</th>
			<th>MEDIO DE PAGO</th>
			<th>FECHA DE OPERACIÃ“N</th>
			<th>CANTIDAD DE ITEMS</th>
			<th>CANTIDAD DE PRESUPUESTOS</th>
			<th>MONEDA</th>
			<th>CANTIDAD DE ETIQUETAS</th>
			<th>OPCIONES</th>
		</tr>
		{{#each egresos}}
		<tr>
			<td>Egreso {{id}}</td>
			<td>{{proveedor.nombreORazonSocial}}</td>
			<td>{{docComercial.tipo}}</td>
			<td>{{medioPago.tipo}}</td>
			<td>{{fechaOperacion}}</td>
			<td>{{cantidadItems}}</td>
			<td>{{cantidadPresupuestos}}</td>
			<td>{{moneda.cadenaIdentificadora}}</td>
			<td>{{cantidadEtiquetas}}</td>
			<td style="padding: 5px;">
				<a style="text-align:center" href="/egreso/{{id}}">
					<img align="center" class="imagen" src="../images/icons/edit.png">
				</a>
				<a style="text-align:center" href="/egreso/{{id}}/items">
					<img align="center" class="imagen" src="../images/icons/items.png">
				</a>
				<a style="text-align:center" href="/egreso/{{id}}/presupuestos">
					<img align="center" class="imagen" src="../images/icons/presupuestos.png">
				</a>
			</td>
		</tr>
		{{/each}}
	</table>
	<br>&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp
	<a href="/egreso" class="nuevoEgreso">Nuevo Egreso</a><br><br>


{{/partial}}

{{>layout.hbs}}